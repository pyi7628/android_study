<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yeongin.lotto" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/money"
        android:label="@string/app_name"
        android:roundIcon="@drawable/money"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.yeongin.lotto.AbsoluteActivity" >
        </activity>
        <activity android:name="com.yeongin.lotto.TestActivity" >
        </activity>
        <activity android:name="com.yeongin.lotto.ResultActivity" android:theme="@style/AppTheme.Result" />
        <activity android:name="com.yeongin.lotto.NameActivity" android:theme="@style/AppTheme.Name"/>
        <activity android:name="com.yeongin.lotto.ConstellationActivity" android:theme="@style/AppTheme.Constellation"/>
        <activity android:name="com.yeongin.lotto.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>